{
  "name": "feathers-schema",
  "version": "0.1.14",
  "description": "Model structure/validation for feathers.js",
  "main": "lib",
  "scripts": {
    "test": "mocha --require babel-register --require babel-polyfill 'test/**/*test.js'",
    "test:watch": "mocha --require babel-register --watch --require babel-polyfill 'test/**/*test.js'",
    "build": "rm -rf lib; mkdir lib; node_modules/.bin/babel src --out-dir lib --plugins transform-runtime",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenZed/feathers-schema.git"
  },
  "keywords": [
    "feathers",
    "schema",
    "model",
    "validation",
    "validate",
    "sanitization",
    "sanitize"
  ],
  "author": "Ben Gaumond",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenZed/feathers-schema/issues"
  },
  "homepage": "https://github.com/BenZed/feathers-schema#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "cli-clear": "^1.0.4",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "feathers": "^2.1.0",
    "feathers-configuration": "^0.4.1",
    "feathers-hooks": "^1.8.0",
    "feathers-memory": "^1.1.0",
    "feathers-rest": "^1.7.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "bson-objectid": "^1.1.5",
    "buffer": "^5.0.5",
    "feathers-errors": "^2.5.0",
    "is-explicit": "^1.1.0"
  }
}
