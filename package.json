{
  "name": "feathers-schema",
  "version": "0.1.0",
  "description": "Model structure/validation for feathers.js",
  "main": "lib",
  "scripts": {
    "test": "mocha --bail --require babel-register --require babel-polyfill 'test/**/*test.js'",
    "test:watch": "mocha --bail --require babel-register --watch --require babel-polyfill 'test/**/*test.js'",
    "create-readme": "./node_modules/.bin/jsdoc2md \"src/*.{js,md}\" > api.md; cat api-intro.md api.md > README.md",
    "build": "rm -rf lib; mkdir lib; node_modules/.bin/babel src --out-dir lib",
    "build:watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenGaumond/feathers-schema.git"
  },
  "keywords": [
    "feathers",
    "schema",
    "model",
    "validation"
  ],
  "author": "Ben Gaumond",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenGaumond/feathers-schema/issues"
  },
  "homepage": "https://github.com/BenGaumond/feathers-schema#readme",
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cli-clear": "^1.0.4",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "feathers": "^2.0.3",
    "feathers-configuration": "^0.4.1",
    "feathers-hooks": "^1.7.1",
    "feathers-memory": "^1.0.1",
    "feathers-rest": "^1.6.0",
    "jsdoc-to-markdown": "^2.0.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "bson-objectid": "^1.1.4",
    "buffer": "^5.0.2",
    "deep-freeze": "0.0.1",
    "feathers-errors": "^2.5.0",
    "is-explicit": "^1.1.0"
  }
}
